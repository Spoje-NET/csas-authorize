{
    "name": "spojenet/csas-authorize",
    "description": "CSAS Access token endpoint App",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SpojeNet\\CSas\\": "src/SpojeNet/CSas",
            "SpojeNet\\CSas\\Ui\\": "src/SpojeNet/CSas/Ui"
        }
    },
